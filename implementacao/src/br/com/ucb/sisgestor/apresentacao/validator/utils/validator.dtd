<?xml version="1.0" encoding="UTF-8"?>
<!-- DTD para o validator -->

<!--
Elemento raiz
-->
<!ELEMENT validator-mapping (
    (validacao)+)>

<!--
Define validadores para as actions
-->
<!ELEMENT validacao EMPTY>
<!--
@attr action Action relacionada.
@attr validator Indica o validator da action.
-->
<!ATTLIST validacao
    action CDATA #REQUIRED
    validator CDATA #REQUIRED
>